<mapper namespace = "mapper.UserMapper">

    <select id = "selectList" resultType = "pojo.User" paramterType = "pojo.User">
        select * from user
    </select>

    <select id="selectOne" resultType="pojo.User" paramterType = "pojo.User">
        select * from user where id = #{id} and username = #{username}
    </select>
</mapper>